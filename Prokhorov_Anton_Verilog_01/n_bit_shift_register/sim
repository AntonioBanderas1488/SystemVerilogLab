:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5a31999df1b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5a31999df340 .scope module, "tb_sr" "tb_sr" 3 1;
 .timescale 0 0;
P_0x5a31999df4d0 .param/l "MSB" 0 3 3, +C4<00000000000000000000000000010000>;
v0x5a31999f5e60_0 .var "clk", 0 0;
v0x5a31999f5f20_0 .var "data", 0 0;
v0x5a31999f5fc0_0 .var "dir", 0 0;
v0x5a31999f60c0_0 .var "en", 0 0;
v0x5a31999f6190_0 .net "out", 15 0, v0x5a31999f5bb0_0;  1 drivers
v0x5a31999f6280_0 .var "rstn", 0 0;
S_0x5a31999a3cf0 .scope module, "sr0" "shift_reg" 3 13, 4 1 0, S_0x5a31999df340;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /INPUT 1 "dir";
    .port_info 4 /INPUT 1 "rstn";
    .port_info 5 /OUTPUT 16 "out";
P_0x5a31999a3ed0 .param/l "MSB" 0 4 1, +C4<00000000000000000000000000010000>;
v0x5a31999ccb00_0 .net "clk", 0 0, v0x5a31999f5e60_0;  1 drivers
v0x5a31999ccd90_0 .net "d", 0 0, v0x5a31999f5f20_0;  1 drivers
v0x5a31999f5a50_0 .net "dir", 0 0, v0x5a31999f5fc0_0;  1 drivers
v0x5a31999f5af0_0 .net "en", 0 0, v0x5a31999f60c0_0;  1 drivers
v0x5a31999f5bb0_0 .var "out", 15 0;
v0x5a31999f5ce0_0 .net "rstn", 0 0, v0x5a31999f6280_0;  1 drivers
E_0x5a31999dd300 .event posedge, v0x5a31999ccb00_0;
    .scope S_0x5a31999a3cf0;
T_0 ;
    %wait E_0x5a31999dd300;
    %load/vec4 v0x5a31999f5ce0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5a31999f5bb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5a31999f5af0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5a31999f5a50_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x5a31999f5bb0_0;
    %parti/s 15, 0, 2;
    %load/vec4 v0x5a31999ccd90_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5a31999f5bb0_0, 0;
    %jmp T_0.6;
T_0.5 ;
    %load/vec4 v0x5a31999ccd90_0;
    %load/vec4 v0x5a31999f5bb0_0;
    %parti/s 15, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5a31999f5bb0_0, 0;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5a31999f5bb0_0;
    %assign/vec4 v0x5a31999f5bb0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5a31999df340;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x5a31999f5e60_0;
    %inv;
    %store/vec4 v0x5a31999f5e60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5a31999df340;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a31999f5e60_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a31999f60c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a31999f5fc0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a31999f6280_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5a31999f5f20_0, 0;
    %end;
    .thread T_2;
    .scope S_0x5a31999df340;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a31999f6280_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5a31999f6280_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5a31999f60c0_0, 0;
    %pushi/vec4 7, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5a31999dd300;
    %load/vec4 v0x5a31999f5f20_0;
    %inv;
    %assign/vec4 v0x5a31999f5f20_0, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5a31999f5fc0_0, 0;
    %pushi/vec4 7, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5a31999dd300;
    %load/vec4 v0x5a31999f5f20_0;
    %inv;
    %assign/vec4 v0x5a31999f5f20_0, 0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %pushi/vec4 7, 0, 32;
T_3.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.5, 5;
    %jmp/1 T_3.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5a31999dd300;
    %jmp T_3.4;
T_3.5 ;
    %pop/vec4 1;
    %vpi_call/w 3 53 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5a31999df340;
T_4 ;
    %vpi_call/w 3 58 "$monitor", "rstn=%0b data=%b, en=%0b, dir=%0b, out=%b", v0x5a31999f6280_0, v0x5a31999f5f20_0, v0x5a31999f60c0_0, v0x5a31999f5fc0_0, v0x5a31999f6190_0 {0 0 0};
    %vpi_call/w 3 59 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call/w 3 61 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a31999df340 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
